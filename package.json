    "@types/chance": "1.1.6",
    "@types/node": "22.1.0",
    "@vitejs/plugin-vue": "5.1.2",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/tsconfig": "0.5.1",
    "eslint": "9.8.0",
    "eslint-plugin-vue": "9.27.0",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "sass-loader": "16.0.0",
    "typescript": "5.5.4",
    "vite": "5.3.5",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-tsc": "2.0.29"
