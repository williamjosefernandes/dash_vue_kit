{
  "name": "able-pro-vuetify-vuejs",
  "version": "1.4.0",
  "private": true,
  "author": "Phoenixcoded",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-stage": "vue-tsc --noEmit && vite build --base=/vue/stage/",
    "build-prod": "vue-tsc --noEmit && vite build --base=/vue/",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore"
  },
  "dependencies": {
    "@eslint/js": "9.8.0",
    "@fontsource/inter": "5.0.20",
    "@fontsource/poppins": "5.0.14",
    "@fontsource/public-sans": "5.0.18",
    "@fontsource/roboto": "5.0.14",
    "@mdi/js": "^7.4.47",
    "@tiptap/starter-kit": "2.5.8",
    "@tiptap/vue-3": "2.5.8",
    "@tsconfig/node20": "20.1.4",
    "@typescript-eslint/parser": "8.0.0",
    "pinia": "2.2.0",
    "resolve-url-loader": "^5.0.0",
    "typescript-eslint": "8.0.0",
    "vee-validate": "4.13.2",
    "vite-plugin-vuetify": "2.0.4",
    "vue": "3.4.35",
    "vue-i18n": "9.13.1",
    "vue-router": "4.4.2",
    "vue-tabler-icons": "2.21.0",
